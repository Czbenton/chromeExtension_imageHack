{
  "manifest_version": 2,
  "name": "Image Hack",
  "description": "Replace all html elements that have no children with an image...Just for fun!",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
